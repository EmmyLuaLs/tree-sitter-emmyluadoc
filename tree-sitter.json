{
  "grammars": [
    {
      "name": "emmyluadoc",
      "camelcase": "EmmyLuaDoc",
      "scope": "comment.block.documentation.lua",
      "path": ".",
      "file-types": [
        "lua"
      ],
      "injection-regex": "^emmyluadoc$",
      "external-files": [],
      "highlights": [
        "queries/highlights.scm"
      ],
      "injections": [
        "queries/injections.scm"
      ],
      "locals": [
        "queries/locals.scm"
      ],
      "tags": [
        "queries/tags.scm"
      ]
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Tree-sitter grammar for EmmyLua documentation comments",
    "authors": [],
    "links": {
      "repository": "https://github.com/EmmyLuaLS/tree-sitter-emmyluadoc"
    }
  }
}
